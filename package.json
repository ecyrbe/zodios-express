    "@types/jest": "29.5.5",
    "@types/node": "20.8.9",
    "@types/supertest": "2.0.14",
    "@zodios/core": "10.9.6",
    "axios": "1.5.1",
    "express": "4.18.2",
    "jest": "29.7.0",
    "rimraf": "5.0.10",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsup": "6.3.0",
    "typescript": "5.2.2",
    "zod": "3.22.4"
  }
